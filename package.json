{
  "name": "@vibe-kit/grokinou-cli",
  "version": "0.1.0",
  "description": "Grokinou - Enhanced fork of grok-cli with multi-provider AI support (Grok, Claude, OpenAI, Mistral, DeepSeek), advanced session management, and modern features",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "grokinou-cli": "dist/index.js",
    "grok": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:bun": "bun run tsc",
    "dev": "bun run src/index.ts",
    "dev:node": "tsx src/index.ts",
    "start": "node dist/index.js",
    "start:bun": "bun run dist/index.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "install:bun": "bun install"
  },
  "keywords": [
    "cli",
    "agent",
    "text-editor",
    "grok",
    "grokinou",
    "ai",
    "multi-provider",
    "openai",
    "claude",
    "mistral",
    "deepseek",
    "session-management"
  ],
  "author": "Zack, Claude, ChatGPT, Grok",
  "license": "BSD-3-Clause AND GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kenchan1111/Grokinou.git"
  },
  "homepage": "https://github.com/Kenchan1111/Grokinou",
  "bugs": {
    "url": "https://github.com/Kenchan1111/Grokinou/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/image-size": "^0.7.0",
    "axios": "^1.7.0",
    "better-sqlite3": "^12.4.1",
    "cfonts": "^3.3.0",
    "chalk": "^5.3.0",
    "clipboardy": "^5.0.1",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "image-size": "^2.0.2",
    "ink": "^4.4.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "openai": "^5.10.1",
    "react": "^18.3.1",
    "ripgrep-node": "^1.0.0",
    "tiktoken": "^1.0.21"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.0",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true
}
